{
    "name": "drupal/drupal",
    "type": "project",
    "require": {
        "cweagans/composer-patches": "^1.6.0",
        "drupal/admin_toolbar": "^1.23",
        "drupal/back_to_top": "^1.0@beta",
        "drupal/bootstrap": "^3.11",
        "drupal/bootstrap_barrio": "^4.7",
        "drupal/console": "^1.0.2",
        "drupal/context": "^4.0@beta",
        "drupal/core": "^8.5",
        "drupal/exif": "^1.0",
        "drupal/link_attributes": "^1.2",
        "drupal/nexus": "^1.1",
        "drupal/onepage_zymphonies_theme": "^1.2",
        "drupal/onescreen": "^1.0",
        "drupal/parallax_bg": "^1.0",
        "drupal/pathauto": "^1.1",
        "drupal/pathologic": "1.x-dev",
        "drupal/photo": "1.x-dev",
        "drupal/redirect": "^1.1",
        "drupal/single_page_site": "^1.4",
        "drush/drush": "^9.2",
        "easyrdf/easyrdf": "^0.9.1",
        "hussainweb/drupal-composer-helper": "^1.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "authors": [
        {
            "name": "Krista Trovato",
            "email": "krista.trovato@me.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "drupal-composer-helper": {
            "web-prefix": "web"
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ]
        }
    },
    "prefer-stable": true,
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
